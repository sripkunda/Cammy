{
    "google": {
        "base": "https://www.google.com/calendar/render?action=TEMPLATE",
        "format": {
            "text": {
                "required": true,
                "param": "text",
                "capitalization": "title"
            },
            "dates": {
                "required": true,
                "param": "dates"
            }, 
            "details": {
                "required": false,
                "param": "details",
                "capitalization": "paragraph"
            },
            "recur": {
                "required": false,
                "param": "recur"
            },
            "location": {
                "required": false,
                "param": "location"
            }
        }
    }
}
